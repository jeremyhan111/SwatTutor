<h1>Listing posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Subject</th>
      <th>Author</th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td><%= post.subject.name %></td>
        <td><%= post.user.name %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
